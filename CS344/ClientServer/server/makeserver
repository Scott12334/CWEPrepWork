	gcc -c HandleFileTransferClient.c

NotesManager.o: NotesManager.c
	gcc -c NotesManager.c

AccountManager.o: AccountManager.c
	gcc -c AccountManager.c

DieWithError.o:	DieWithError.c
	gcc -c DieWithError.c

