serverWithMenu:	serverWithMenu.o HandleTCPClientWithMenu.o DieWithError.o getPut.o
	gcc serverWithMenu.o HandleTCPClientWithMenu.o DieWithError.o getPut.o -o serverWithMenu

serverWithMenu.o:	serverWithMenu.c
	gcc -c serverWithMenu.c

